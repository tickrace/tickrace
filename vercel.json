{
  "rewrites": [
    {
      "source": "/courses/:id",
      "has": [{ "type": "query", "key": "app", "value": "1" }],
      "destination": "/index.html"
    },
    {
      "source": "/courses/:id",
      "destination": "/api/prerender/course/:id"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
